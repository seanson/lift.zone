{
  "name": "lift.zone",
  "version": "1.0.0",
  "description": "Website to parse your workouts",
  "main": "server.js",
  "scripts": {
    "start": "watch 'npm run-script templates' templates & beefy client/app.js:js/bundle.js 8080 --index=./public/index.html --cwd=./public",
    "build": "mv config.js local-config.js && mv build-config.js config.js && templatizer -d ./templates -o client/templates.js && browserify client/app.js -o public/js/bundle.js --debug && mv config.js build-config.js && mv local-config.js config.js",
    "minify": "cat public/js/bundle.js | uglifyjs -o public/js/bundle.js",
    "templates": "templatizer -d ./templates -o client/templates.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wraithgar/lift.zone.git"
  },
  "author": "Gar <gar+code@danger.computer>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "ampersand-app": "^1.0.0",
    "ampersand-collection": "^1.3.17",
    "ampersand-dom": "^1.2.7",
    "ampersand-grouped-collection-view": "^1.1.3",
    "ampersand-model": "^4.0.3",
    "ampersand-rest-collection": "^2.0.4",
    "ampersand-router": "^1.0.6",
    "ampersand-state": "^4.4.4",
    "ampersand-view": "^7.2.0",
    "ampersand-view-switcher": "^2.0.0",
    "andlog": "^1.0.0",
    "beefy": "^2.1.3",
    "browserify": "^8.1.3",
    "caber": "^4.0.4",
    "domready": "^1.0.7",
    "jade": "^1.9.2",
    "jshint": "^2.5.11",
    "lodash.debounce": "^3.0.1",
    "moment": "^2.9.0",
    "precommit-hook": "^1.0.7",
    "templatizer": "^1.1.1",
    "uglify-js": "^2.4.16",
    "watch": "^0.14.0",
    "xhr": "^1.10.0"
  }
}
