{
  "name": "lift.zone",
  "version": "1.0.0",
  "description": "Website to parse your workouts",
  "main": "server.js",
  "scripts": {
    "start": "beefy client/app.js:js/bundle.js 8080 --index=./public/index.html --cwd=./public -- -t [ jadeify --doctype html ]",
    "build": "node build-static && mv config.js local-config.js && mv build-config.js config.js && browserify client/app.js -t [ jadeify --doctype html ] -p [ minifyify --no-map ] -o public/js/bundle.js && mv config.js build-config.js && mv local-config.js config.js",
    "lint": "eslint client",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wraithgar/lift.zone.git"
  },
  "author": "Gar <gar+code@danger.computer>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "ampersand-app": "^2.0.0",
    "ampersand-collection": "^2.0.0",
    "ampersand-dom": "^1.5.0",
    "ampersand-grouped-collection-view": "^2.0.0",
    "ampersand-model": "^8.0.0",
    "ampersand-rest-collection": "^6.0.0",
    "ampersand-router": "^4.0.0",
    "ampersand-state": "^5.0.2",
    "ampersand-sync": "^5.0.0",
    "ampersand-view": "^10.0.1",
    "ampersand-view-switcher": "^2.1.0",
    "beefy": "^2.1.8",
    "browserify": "^13.1.0",
    "caber": "^4.0.10",
    "debug": "^2.2.0",
    "domready": "^1.0.8",
    "eslint": "^3.2.0",
    "eslint-config-hapi": "^9.1.0",
    "eslint-plugin-hapi": "^4.0.0",
    "git-validate": "^2.1.4",
    "jade": "^1.11.0",
    "jadeify": "^4.6.0",
    "local-links": "^1.4.0",
    "lodash.debounce": "^4.0.7",
    "lodash.foreach": "^4.4.1",
    "minifyify": "^7.3.3",
    "moment": "^2.14.1",
    "xhr": "^2.2.2"
  },
  "pre-commit": [
    "lint",
    "validate"
  ],
  "devDependencies": {
    "eslint": "^3.2.2"
